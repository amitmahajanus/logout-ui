// App.js
import React from 'react';
import './App.css';

function App() {
  const handleCloseTab = () => {
    window.close();
  };

  return (
    <div className="App">
      <header className="App-header">
        <h1>Close Tab Demo</h1>
        <button onClick={handleCloseTab}>Close Tab</button>
      </header>
    </div>
  );
}

export default App;



